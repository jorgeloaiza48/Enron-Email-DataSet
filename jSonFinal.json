   "Message-ID": " \u003c8212312.1075855696560.JavaMail.evans@thyme\u003e",
   "Date": " Thu, 8 Mar 2001 05:33:00 -0800 (PST)",
   "from": " Phillip K Allen",
   "to": "  \"George Richards\" \u003ccbpres@austin.rr.com\u003e",
   "subject": "  Sagewood Phase II",
   "Mime-Version": "ion: 1.0",
   "Content-Type": "ype: text/plain; charset=us-ascii",
   "Content-Transfer-Encoding": "ransfer-Encoding: 7bit",
   "X-From": "",
   "X-To": "",
   "X-cc": "",
   "X_bcc": " ",
